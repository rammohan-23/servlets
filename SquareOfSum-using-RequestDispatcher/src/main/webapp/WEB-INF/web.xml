<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>SquareOfSum-using-RequestDispatcher</display-name>
  <servlet>
    <servlet-name>addSrvlt</servlet-name>
    <servlet-class>com.rammohan.servlets.add.square.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addSrvlt</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>sqSrvlt</servlet-name>
    <servlet-class>com.rammohan.servlets.add.square.SqServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sqSrvlt</servlet-name>
    <url-pattern>/sq</url-pattern>
  </servlet-mapping>
</web-app>